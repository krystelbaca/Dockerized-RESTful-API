{
  "name": "dockerized-node-app",
  "version": "1.0.0",
  "description": "CRUD app example on Docker",
  "author": "Krystel Baca Salayandia",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jasmine",
    "db:init": "node 'database/scripts/create.db.schema.js'",
    "db:migrations": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all"
  },
  "dependencies": {
    "agent": "^0.2.1",
    "express": "^4.17.1",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "request": "^2.88.2",
    "sequelize": "^5.21.6",
    "standard": "^14.3.4",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "jasmine": "^3.6.1",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^6.2.0"
  },
  "standard": {
    "globals": [
      "jasmine",
      "describe",
      "it",
      "fit",
      "expect",
      "beforeAll"
    ]
  }
}
